{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "installCommand": "pnpm install",
  "buildCommand": "pnpm run build",
  "rewrites": [
    {
      "source": "/admin/:path*",
      "destination": "https://payload-sigma-ten.vercel.app/admin/:path*"
    },
    {
      "source": "/media/:path*",
      "destination": "https://payload-sigma-ten.vercel.app/media/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "https://payload-sigma-ten.vercel.app/api/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.svg",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    }
  ],
  "github": {
    "silent": true,
    "autoAlias": true,
    "autoJobCancelation": true
  }
}
