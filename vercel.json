{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "installCommand": "pnpm install",
  "buildCommand": "pnpm run build",
  "routes": [
    {
      "src": "/admin/(.*)",
      "dest": "https://payload-sigma-ten.vercel.app/admin/$1"
    }
  ],
  "headers": [
    {
      "source": "/(logo\\.svg|white-logo\\.svg|favicon.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    }
  ],
  "github": {
    "silent": true,
    "autoAlias": true,
    "autoJobCancelation": true
  }
}
